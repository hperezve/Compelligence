  <where>
    <![CDATA[
    PositioningByCompetitorAndIndustryView.ClientCompany = '$S{ClientCompany}' And PositioningByCompetitorAndIndustryView.EntityType = '$C{DomainObjectType.Competitor}'
    ]]>
  </where>
  <orderBy>
