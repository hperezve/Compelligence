    </column>
    <column>
      <entity>NewsByCompetitorView</entity>
      <property>AssignedToName</property>
      <label>LibraryAssignedToNameReport</label>
    </column>
    <column>
      <entity>NewsByCompetitorView</entity>
      <property>Score</property>
      <label>LibraryScoreReport</label>
    </column>
  </select>
  <from>
    <![CDATA[
    Compelligence.Domain.Entity.Views.NewsByCompetitorView as NewsByCompetitorView
    ]]>
  </from>
  <where>
    <![CDATA[
    NewsByCompetitorView.ClientCompany = '$S{ClientCompany}' AND NewsByCompetitorView.CompetitorId is not null AND NewsByCompetitorView.EntityType = '$C{DomainObjectType.Competitor}'
    ]]>
  </where>
  <orderBy>
    <![CDATA[
    NewsByCompetitorView.DateAdded
    ]]>
  </orderBy>
</browse>