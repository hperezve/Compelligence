<?xml version="1.0" encoding="utf-8" ?>
<browse>
  <select>
    <column>
      <entity>ProjectsbyCustomerView</entity>
      <property>Name</property>
      <label>ProjectNameReport</label>
    </column>
    <column>
      <entity>ProjectsbyCustomerView</entity>
      <property>CustomerName</property>
      <label>ProjectsbyCustomerViewCustomerName</label>
    </column>
    <column>
      <entity>ProjectsbyCustomerView</entity>
      <property>IndustryName</property>
      <label>IndustryNameReport</label>
    </column>
    <column>
      <entity>ProjectsbyCustomerView</entity>
      <property>CompetitorName</property>
      <label>CompetitorNameReport</label>
    </column>
    <column>
      <entity>ProjectsbyCustomerView</entity>
      <property>ProductName</property>
      <label>ProductNameReport</label>
    </column>
    <column>
      <entity>ProjectsbyCustomerView</entity>
      <property>Status</property>
      <label>ProjectStatus</label>
      <type class="StandardData">ProjectStatus</type>
    </column>
    <column>
      <entity>ProjectsbyCustomerView</entity>
      <property>AssignedToName</property>
      <label>ProjectAssignedToName</label>
    </column>
  </select>
  <from>
    <![CDATA[
    Compelligence.Domain.Entity.Views.ProjectsbyCustomerView as ProjectsbyCustomerView
    ]]>
  </from>
  <where>
    <![CDATA[
    ProjectsbyCustomerView.ClientCompany = '$S{ClientCompany}' AND ProjectsbyCustomerView.CustomerId is not null
    ]]>
  </where>
  <orderBy>
    <![CDATA[
    ProjectsbyCustomerView.CustomerName
    ]]>
  </orderBy>
  <onSelectRow>
    <![CDATA[
    getEntity('$P{urlEdit}', '$P{scope}', '$P{controller}', id, '$P{browseId}');
    ]]>
  </onSelectRow>
  <loadComplete>
    <![CDATA[
    hideLoadingDialog();
    ]]>
  </loadComplete>
</browse>