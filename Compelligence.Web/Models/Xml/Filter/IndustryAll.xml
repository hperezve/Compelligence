    </column>
  </select>
  <from>
    <![CDATA[
    Compelligence.Domain.Entity.Views.IndustryAllView as IndustryAllView
    ]]>
  </from>
  <where>
    <![CDATA[
   IndustryAllView.ClientCompany = '$S{ClientCompany}'
    ]]>
  </where>
  <orderBy>
    <![CDATA[
    IndustryAllView.Id
    ]]>
  </orderBy>
  <onSelectRow>
    <![CDATA[
    getEntity('$P{urlEdit}', '$P{scope}', '$P{controller}', id, '$P{browseId}');
    ]]>
  </onSelectRow>
  <loadComplete>
    <![CDATA[
    hideLoadingDialog();
    ]]>
