    </column>
    <column group="true">
      <entity>CompetitorAllView</entity>
      <property>Tier</property>
      <label>CompetitorTier</label>
      <type class="StandardData">CompetitorTier</type>
    </column>
    <column>
      <entity>CompetitorAllView</entity>
      <property>Name</property>
      <label>CompetitorName</label>
    </column>
    <column>
      <entity>CompetitorAllView</entity>
      <property>Description</property>
      <label>CompetitorDescription</label>
    </column>
    <column>
      <entity>CompetitorAllView</entity>
      <property>Phone</property>
      <label>CompetitorPhone</label>
    </column>
    <column>
      <entity>CompetitorAllView</entity>
