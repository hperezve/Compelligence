      <property>ImageUrl</property>
      <label>CompetitorImageUrl</label>
    </column>
    <column>
      <entity>CompetitorAllView</entity>
      <property>KeyWord</property>
      <label>CompetitorKeyWord</label>
    </column>
  </select>
  <from>
    <![CDATA[
    Compelligence.Domain.Entity.Views.CompetitorAllView as CompetitorAllView 
    ]]>
  </from>
  <where>
    <![CDATA[
   CompetitorAllView.ClientCompany = '$S{ClientCompany}'
    ]]>
  </where>
  <orderBy>
    <![CDATA[
    CompetitorAllView.Name
    ]]>
  </orderBy>
  <onSelectRow>
    <![CDATA[
    getEntity('$P{urlEdit}', '$P{scope}', '$P{controller}', id, '$P{browseId}');
    ]]>
  </onSelectRow>
  <loadComplete>
    <![CDATA[
    hideLoadingDialog();
    ]]>
  </loadComplete>
</browse>
