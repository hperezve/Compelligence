   IndustryByParentView.ClientCompany = '$S{ClientCompany}' and (IndustryByParentView.Status ='$C{IndustryStatus.Enabled}' OR IndustryByParentView.Status ='$C{IndustryStatus.BackEndOnly}')
    ]]>
